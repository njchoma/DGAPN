    "version": "0.2.0",
    "configurations": [
        {
            "name": "main_predict_dock_score",
            "type": "python",
            "request": "launch",
            "program": "src/main_predict_dock_score.py",
            "subProcess": true,
            "args": [
                "--name", "predict_dock_score",
                "--use_3d",
                "--data_path", "src/dataset/NSP15_6W01_A_1_F.Orderable_zinc_db_enaHLL.2col.csv",
                "--artifact_path", "test_run/dgapn",
                "--gpu", "0"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "main_dgapn",
            "type": "python",
            "request": "launch",
            "program": "src/main_dgapn.py",
            "subProcess": true,
            "args": [
                "--name", "DGAPN",
                "--data_path", "src/dataset",
                "--artifact_path", "test_run/dgapn",
                "--warm_start_dataset_path", "src/dataset/NSP15_6W01_A_1_F.Orderable_zinc_db_enaHLL.2col.csv",
                "--surrogate_model_path", "artifact/test_surrogate.pth",
                "--gpu", "0"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "main_evaluate",
            "type": "python",
            "request": "launch",
            "program": "src/main_evaluate.py",
            "subProcess": true,
            "args": [
                "--name", "eval",
                //"--greedy",
                "--data_path", "src/dataset",
                "--artifact_path", "test_run/dgapn",
                "--warm_start_dataset_path", "src/dataset/NSP15_6W01_A_1_F.Orderable_zinc_db_enaHLL.2col.csv",
                "--surrogate_guide_path", "artifact/test_surrogate.pth",
