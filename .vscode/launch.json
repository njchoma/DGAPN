    "version": "0.2.0",
    "configurations": [
        {
            "name": "main_embed",
            "type": "python",
            "request": "launch",
            "program": "src/main_embed.py",
            "subProcess": true,
            "args": [
                "--name", "embed",
                //"--use_3d",
                "--data_path", "src/dataset/NSP15_6W01_A_3_H.negonly_unique.csv",
                "--artifact_path", "test_run/dgapn",
                "--gpu", "0"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "main_dgapn",
            "type": "python",
            "request": "launch",
            "program": "src/main_dgapn.py",
            "subProcess": true,
            "args": [
                "--name", "DGAPN",
                "--data_path", "src/dataset",
                "--artifact_path", "test_run/dgapn",
                "--reward_type", "logp", // change to dock
                "--warm_start_dataset_path", "src/dataset/NSP15_6W01_A_3_H.negonly_unique.csv",
                "--embed_model_path", "artifact/test_embed.pth",
                "--gpu", "0"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "main_evaluate",
            "type": "python",
            "request": "launch",
            "program": "src/main_evaluate.py",
            "subProcess": true,
            "args": [
                "--name", "eval",
                //"--greedy",
                "--data_path", "src/dataset",
                "--artifact_path", "test_run/dgapn",
                "--reward_type", "logp", // change to dock
                "--warm_start_dataset_path", "src/dataset/NSP15_6W01_A_3_H.negonly_unique.csv",
                "--policy_path", "artifact/test_dgapn.pth"
            ],
            "console": "integratedTerminal"
        }
    ]
}
